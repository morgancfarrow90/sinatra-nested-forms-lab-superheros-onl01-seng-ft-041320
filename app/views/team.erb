<div class="team">
  <h1><%= @team.name %></h1><br>
  <h2>Team Motto: <%= @team.motto %>!</h2><br>
  
</div><br>


<% @members.each do |member| %>
<div class="member">
  <h2>Hero Name: <%= member.name %></h2><br>
  <p>Hero Power: <%= member.power %></p><br>
  <p>Hero Biography: <%= member.bio %></p>
</div><br>
<% end %>


